{{#if tag}}#[serde(tag = "{{tag}}")]{{/if}}
pub enum {{name}} {
{{~#each variants}}
  #[serde(rename = "{{rename}}")]
  {{name}}{{#if value}}({{value}}){{/if}},
{{~/each}}
}
{{~/each}}
{{~#each structs}}
