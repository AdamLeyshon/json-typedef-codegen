class {{ enum_.name }}(Enum):
{{ enum_.metadata|description(1) }}
{% for variant in enum_.variants %}
    {{ variant.name }} = {{ "{:?}"|format(variant.json_value) }}
{{ variant.metadata|enum_variant_description(1, variant.json_value) }}
{% endfor %}

    @classmethod
    def from_json(cls, data) -> {{ "{:?}"|format(enum_.name) }}:
        """
        Construct an instance of this class from parsed JSON data.
        """

        return cls(data)

    def to_json(self):
        """
        Generate JSON-ready data from an instance of this class.
        """

        return self.value
